.**1. ROLLE****************************************************************
.if x&DEP01ROLT = x03
.th .se rettxt1 = 'Rettighedstekst'
.el .se rettxt1 = ' '
.*******************
.if x&DEP01ROLT = x02
.th .se rol1 = 'Medejer'
.if x&DEP01ROLT = x03
.th .se rol1 = 'Rettighedshaver'
