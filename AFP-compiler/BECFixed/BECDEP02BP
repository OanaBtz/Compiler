.th .se rol1 = 'Indskyder'
.if x&DEP01ROLT = x06
.th .se rol1 = 'Fuldmagtshaver'
.if x&DEP01ROLT = x07
.th .se rol1 = 'Værge'
.if x&DEP01ROLT = x08
.th .se rol1 = 'Andre'
