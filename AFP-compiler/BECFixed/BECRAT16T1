.** test ratepensionstype **
.if &L'&GENARBFAST > 0
.th .go TEST2
.if &L'&GENARBPCT  > 0
.th .go TEST4
.if &L'&GENARBENGA > 0
.th .go OKTEST
.el .mg /T/Ratepensionstype skal vælges/
...TEST2
.if &L'&GENARBPCT  > 0
.or &L'&GENARBENGA > 0
.th .go FEJL
.el .go OKTEST
...TEST4
.if &L'&GENARBENGA > 0
.th .go FEJL
.el .go OKTEST
...FEJL
.th .mg /T/Højst een ratepensionstype må vælges/
...OKTEST
.** test arbgiv navn **
.if &L'&GENNAVN1 eq 0
.an &L'&GENGADE1 eq 0
.an &L'&GENBY    eq 0
.an &L'&GENSENR  eq 0
.th .mg /T/Arbejdsgiveroplysninger mangler/
.** test arbgiv beløb **
.if &GENARB gt 0,00
.or &GENMED gt 0,00
.th .go ARBGIVSL
.el .mg /T/Arb.givers del eller medarbejders del skal indtastes/
...ARBGIVSL
.** test frekvens **
.if &L'&GENARBENGA > 0
.th .go EJFREKV
.if &L'&GENLØNINDB = 0
.th .mg /T/Frekvens skal vælges/
.if x&GENLØNINDB < x1
.or x&GENLØNINDB > x6
.th .mg /T/Frekvens skal have værdien 1-6/
.if &L'&GENARBPCT > 0
.th .go EJFREKV
.if &L'&GENARBFAST > 0
.th .go NYTEST
.* kun test for genarbfast(fast)
...NYTEST
.if x&GENLØNINDB ne x1
.th .go NYAARSL
.if &L'&RESULTA gt 12
.th .mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
.if &L'&RESULTA lt 12
.th .go EJFREKV
.if X&RESULTA gt X&GENMAX15B
.th .mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
.el .go EJFREKV
...NYAARSL
.*
.if x&GENLØNINDB ne x2
.th .go NYHALVSL
.if &L'&RESULTH gt 12
.th .mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
.if &L'&RESULTH lt 12
.th .go EJFREKV
.if X&RESULTH gt X&GENMAX15B
.th .mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
.el .go EJFREKV
...NYHALVSL
.if x&GENLØNINDB ne x3
.th .go NYKVASL
.if &L'&RESULTB gt 12
.th .mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
.if &L'&RESULTB lt 12
.th .go EJFREKV
.if X&RESULTB gt X&GENMAX15B
.th .mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
.el .go EJFREKV
...NYKVASL
.if x&GENLØNINDB ne x4
.th .go NYMDRSL
.if &L'&RESULTC gt 12
.th .mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
.if &L'&RESULTC lt 12
.th .go EJFREKV
.if X&RESULTC gt X&GENMAX15B
.th .mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
.el .go EJFREKV
...NYMDRSL
.if x&GENLØNINDB ne x5
.th .go NY14DASL
.if &L'&RESULTD gt 12
.th .mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
.if &L'&RESULTD lt 12
.th .go EJFREKV
.if X&RESULTD gt X&GENMAX15B
.th .mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
.el .go EJFREKV
...NY14DASL
.if x&GENLØNINDB ne x6
.th .go EJFREKV
.if &L'&RESULTC gt 12
.th .mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
.if &L'&RESULTC lt 12
.th .go EJFREKV
.if X&RESULTC gt X&GENMAX15B
.th .mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
.el .go EJFREKV
...EJFREKV
