.if &L'&GENARBFAST > 0
.th .go INDENPRI
.if &L'&GENARBPCT  > 0
.th .go UDENTST
.****dette er test på beløb for engangsindskud ********
.if &L'&RESULTA gt 12
.th .go ENGFEJL
.if &L'&RESULTA eq 12
.an X&RESULTA gt X&GENMAX15B.
.th .go ENGFEJL
.el .go UDENTST
...ENGFEJL
.mg /T/BELØB SKAL VÆRE MINDRE END &GENMAX15B./
...INDENPRI
.if &L'&GENPRISTAL eq 0
.th .go UDENTST
.if x&GENPRISTAL < x1
.or x&GENPRISTAL > x4
.th .mg /T/Ved pristalsregulering skal indeks være 1-4/
...UDENTST
